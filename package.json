{
  "name": "matt-react-boilerplate",
  "version": "0.0.1",
  "description": "A opinionated React boilerplate using Webpack, PostCSS and Babel",
  "main": "index.js",
  "repository": "https://github.com/matbueno/react-boilerplate.git",
  "author": "matt <mat.bueno.ferreira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "start": "node server.js",
    "dev": "webpack-dev-server --open --hot --host 0.0.0.0 --config webpack.config.dev.js",
    "storybook": "start-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@storybook/react": "^5.2.6",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss": "^7.0.21",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.13.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "pretty-quick": "^2.0.1",
    "remove-files-webpack-plugin": "^1.1.3",
    "stylelint": "^11.1.1",
    "stylelint-config-css-modules": "^2.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ],
    "*.{css}": [
      "stylelint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
