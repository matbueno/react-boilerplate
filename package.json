{
  "name": "matt-react-boilerplate",
  "version": "0.0.1",
  "description": "A opinionated React boilerplate using Webpack, PostCSS and Babel",
  "main": "index.js",
  "repository": "https://github.com/matbueno/react-boilerplate.git",
  "author": "matt <mat.bueno.ferreira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "start": "node server.js",
    "dev": "webpack-dev-server --open --hot --host 0.0.0.0 --config webpack.config.dev.js",
    "storybook": "start-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@storybook/react": "^6.1.11",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.3",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "pretty-quick": "^3.1.0",
    "remove-files-webpack-plugin": "^1.4.4",
    "stylelint": "^13.8.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ],
    "*.{css}": [
      "stylelint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
