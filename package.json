{
  "name": "matt-react-boilerplate",
  "version": "0.0.1",
  "description": "A opinionated React boilerplate using Webpack, PostCSS and Babel",
  "main": "index.js",
  "repository": "https://github.com/matbueno/react-boilerplate.git",
  "author": "matt <mat.bueno.ferreira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "start": "node server.js",
    "dev": "webpack-dev-server --open --hot --host 0.0.0.0 --config webpack.config.dev.js",
    "storybook": "start-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@storybook/react": "^5.3.19",
    "autoprefixer": "^9.8.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^7.0.32",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "pretty-quick": "^2.0.1",
    "remove-files-webpack-plugin": "^1.4.3",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ],
    "*.{css}": [
      "stylelint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
